{
  "name": "oxidejs",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxidejs/oxide.git",
    "directory": "packages/framework"
  },
  "files": [
    "dist",
    "components",
    "virtual.d.ts",
    "app.tsconfig.json"
  ],
  "type": "module",
  "module": "src/index.ts",
  "typesVersions": {
    "*": {
      "virtual": [
        "virtual.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./components/LayoutRenderer.svelte": "./components/LayoutRenderer.svelte",
    "./components/ErrorRenderer.svelte": "./components/ErrorRenderer.svelte",
    "./components/OxideProvider.svelte": "./components/OxideProvider.svelte",
    "./client": {
      "types": "./dist/client.d.mts",
      "import": "./dist/client.mjs"
    },
    "./nitro": {
      "types": "./dist/nitro.d.mts",
      "import": "./dist/nitro.mjs"
    },
    "./virtual.d.ts": "./virtual.d.ts",
    "./app.tsconfig.json": "./app.tsconfig.json"
  },
  "scripts": {
    "build": "tsdown"
  },
  "dependencies": {
    "dedent": "1.7.1",
    "rou3": "0.7.12"
  },
  "devDependencies": {
    "@tsconfig/bun": "1.0.10",
    "@types/bun": "1.3.8",
    "nitro": "3.0.1-alpha.2",
    "rollup-plugin-svelte": "7.2.3",
    "svelte-preprocess": "6.0.3",
    "tsdown": "0.20.1",
    "vite": "7.3.1"
  },
  "peerDependencies": {
    "svelte": "^5",
    "typescript": "^5"
  }
}
